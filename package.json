{
  "name": "kiUsecaseExplorerV3",
  "type": "module",
  "scripts": {
    "dev": "wasp start",
    "dev:clean": "wasp clean && wasp start",
    "build": "wasp build",
    "start": "wasp start",
    "lint": "wasp lint || echo 'Wasp lint not available'",
    "type-check": "wasp type-check || echo 'Wasp type-check not available'",
    "deploy": "./scripts/deploy.sh",
    "deploy:azure": "wasp build && ./scripts/deploy.sh",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:unit": "jest --config jest.config.cjs --testPathPatterns=\"src/.*\\.test\\.(ts|tsx)$\"",
    "test:integration": "jest --config jest.config.cjs --testPathPatterns=\"tests/integration/.*\\.test\\.(ts|tsx)$\"",
    "test:e2e": "playwright test",
    "test:setup": "npm run db:migrate",
    "test:ci": "npm run test:unit && npm run test:integration",
    "db:migrate": "wasp db migrate-dev",
    "db:studio": "wasp db studio",
    "db:seed": "tsx tests/seeds/developmentSeed.ts",
    "db:seed:test": "tsx tests/seeds/testSeed.ts",
    "db:seed:staging": "tsx -e \"import {DatabaseSeeder} from './tests/seeds/DatabaseSeeder'; const seeder = new DatabaseSeeder(); seeder.seedStaging().then(() => seeder.disconnect());\"",
    "import:usecases": "tsx scripts/import-usecases.ts",
    "import:sample": "tsx scripts/import-usecases.ts --generate-sample",
    "import:bechtle": "tsx scripts/import-bechtle-usecases.ts",
    "import:bechtle:dry": "tsx scripts/import-bechtle-usecases.ts --dry-run",
    "verify:users": "tsx scripts/verify-all-users.ts",
    "deploy:backend": "./scripts/deploy-backend.sh",
    "deploy:backend:appservice": "./scripts/deploy-backend-appservice.sh"
  },
  "dependencies": {
    "csv-parser": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.26.2",
    "wasp": "file:.wasp/out/sdk/wasp"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "playwright": "^1.53.2",
    "prisma": "5.19.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.1.0",
    "vite": "^4.3.9"
  }
}
